{
    "name": "hroc/laravel-saml2",
    "type": "library",
    "description": "SAML2 Service Provider integration to your Laravel 5.4+ application, based on OneLogin toolkit",
    "keywords": ["laravel", "saml", "saml2", "onelogin", "sso"],
    "homepage": "https://github.com/hroc/laravel-saml2",
    "license": "MIT",
    "authors": [
        {
            "name": "HROC",
            "email": "webmaster@hroc.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-openssl": "*",
        "illuminate/console": "~5.0|~5.5|^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "~5.0|~5.5|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "~5.0|~5.4|^6.0|^7.0|^8.0|^9.0",
        "onelogin/php-saml": "^3.0",
        "ramsey/uuid": "^3.8|^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^7.5|^9.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "HROC\\Saml2\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Saml2": "HROC\\Saml2\\Facades\\Auth"
            },
            "providers": [
                "HROC\\Saml2\\ServiceProvider"
            ]
        },
        "branch-aliases": {
            "dev-master": "2.0.8-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
